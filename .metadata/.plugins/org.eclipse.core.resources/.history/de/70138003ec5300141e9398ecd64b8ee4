#ifndef SAMPLES_H_
#define SAMPLES_H_

#define CANT_MUESTRAS_POR_MUESTREO 10

struct s_samples {

	double samplesRaw[CANT_MUESTRAS_POR_MUESTREO];
	double samplesProcessed[CANT_MUESTRAS_POR_MUESTREO];
	int timesSapling[CANT_MUESTRAS_POR_MUESTREO];
	bool processed;
};

#endif /* SAMPLES_H_ */
